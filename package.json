{
  "name": "npm-install",
  "main": "./index",
  "version": "4.0.4",
  "private": true,
  "description": "Automatically install and save any missing npm modules being used in the current file",
  "activationCommands": {
    "atom-text-editor": [
      "npm-install:save",
      "npm-install:save-dev"
    ]
  },
  "repository": "https://github.com/hughsk/atom-npm-install",
  "license": "MIT",
  "engines": {
    "atom": ">=0.200.0"
  },
  "dependencies": {
    "ansi-html-stream": "0.0.3",
    "ansi-to-html": "^0.3.0",
    "atom-message-panel": "^1.2.4",
    "atom-selected-requires": "^1.1.2",
    "combine-stream": "0.0.4",
    "domify": "^1.3.3",
    "findup": "^0.1.5",
    "relative-require-regex": "^1.0.1",
    "resolve": "1.3.2",
    "split": "^0.3.3",
    "which": "^1.1.1",
    "xtend": "^4.0.0"
  }
}

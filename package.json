{
  "name": "npm-install",
  "main": "./index",
  "version": "1.0.0",
  "private": true,
  "description": "Automatically install and save any missing npm modules being used in the current file",
  "activationEvents": [
    "npm-install:save",
    "npm-install:save-dev"
  ],
  "repository": "https://github.com/hughsk/atom-npm-install",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "ansi-html-stream": "0.0.3",
    "coffee-script": "^1.7.1",
    "detective": "^2.4.1",
    "domify": "^1.2.2",
    "findup": "^0.1.3",
    "map-async": "^0.1.1",
    "npm-stats": "^0.3.0",
    "remove-element": "0.0.0",
    "resolve": "^0.6.1",
    "safe-json-parse": "^1.0.1"
  }
}
